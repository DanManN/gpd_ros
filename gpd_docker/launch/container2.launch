<launch>
  <arg name="config1" default="$(find pracsys_vbnpm)/scripts/grasp_planner/gpd_ros_large_bite.cfg"/>
  <arg name="config2" default="$(find pracsys_vbnpm)/scripts/grasp_planner/gpd_ros_small_bite.cfg"/>
  <node name="gpd_srv" pkg="gpd_docker" type="run_container.py" output="screen" args="-host $(optenv ROS_MASTER localhost) -c $(arg config1) -d $(arg config2)" />
</launch>
